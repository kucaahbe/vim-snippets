snippet #! "#!/usr/bin/env python" b
#!/usr/bin/env python
endsnippet

snippet enc "encoding" b
# -*- coding: UTF-8 -*-
endsnippet

snippet if "if :"
if ${1}:
	${0:pass}
endsnippet

snippet ife "if : else"
if ${1}:
	${2:pass}
else:
	${0:pass}
endsnippet

snippet def "def func():"
def ${1:meth}(${2:self}):
	${0:pass}
endsnippet

snippet dbg "pdb"
import pdb; pdb.set_trace()
endsnippet
